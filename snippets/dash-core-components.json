{
    "Dash Core Component - Checklist": {
        "prefix": "dashcorechecklist",
        "description": "Create a dcc.Checklist element",
        "body": [
			"dcc.Checklist(id='$1', className='$2',",
            "\toptions=[$4],",
			"\tvalue='$5'",
            ")$0"
        ]
	},
	"Dash Core Component - Date Picker Range": {
        "prefix": "dashcoredatepickerrange",
        "description": "Create a dcc.DatePickerRange element",
        "body": [
			"dcc.DatePickerRange(id='$1', className='$2',",
            "\tstart_date=$3,",
			"\tend_date=$4,",
			"\tdisplay_format='$5',",
			"\t#min_date_allowed=$6,",
			"\t#max_date_allowed=$7",
            ")$0"
        ]
	},
	"Dash Core Component - Date Picker Single": {
        "prefix": "dashcoredatepickersingle",
        "description": "Create a dcc.DatePickerSingle element",
        "body": [
			"dcc.DatePickerSingle(id='$1', className='$2',",
			"\tdate=$3,",
			"\tdisplay_format='$4',",
			"\t#min_date_allowed=$5,",
			"\t#max_date_allowed=$6",
            ")$0"
        ]
	},
	"Dash Core Component - Dropdown": {
        "prefix": "dashcoredropdown",
        "description": "Create a dcc.Dropdown element",
        "body": [
            "dcc.Dropdown(id='$1', className='$2',",
            "\toptions=[$2],",
			"\tvalue='$3',",
			"\t#placeholder='$4'",
            ")$0"
        ]
	},
	"Dash Core Component - Graph": {
        "prefix": "dashcoregraph",
        "description": "Create a dcc.Graph element",
        "body": [
			"dcc.Graph(id='$1', className='$2',",
			"\tfigure='$3'",
			")$0"
        ]
	},
	"Dash Core Component - Input": {
        "prefix": "dashcoreinput",
        "description": "Create a dcc.Input element",
        "body": [
			"dcc.Input(id='$1', className='$2',",
			"\ttype='$3',",
            "\tplaceholder='$4',",
			"\tvalue='$5'",
            ")$0"
        ]
	},
	"Dash Core Component - Interval": {
        "prefix": "dashcoreinterval",
        "description": "Create a dcc.Interval element",
        "body": [
			"dcc.Interval(id='$1', interval=$2)$0"
        ]
	},
	"Dash Core Component - Link": {
        "prefix": "dashcorelink",
        "description": "Create a dcc.Link element",
        "body": [
			"dcc.Input(id='$1', className='$2',",
			"\thref='$3',",
            "\tchildren='$4'",
            ")$0"
        ]
	},
	"Dash Core Component - Loading": {
        "prefix": "dashcoreloading",
        "description": "Create a dcc.Loading element",
        "body": [
			"dcc.Loading(id='$1', className='$2', color='$3', children=[",
			"\t$5",
			"])$0"
        ]
	},
	"Dash Core Component - Location": {
        "prefix": "dashcorelocation",
        "description": "Create a dcc.Location element",
        "body": [
			"dcc.Location(id='$1', className='$2', refresh=True$3,",
			"\thref='$4', pathname='$5')$0"
        ]
	},
	"Dash Core Component - Markdown": {
        "prefix": "dashcoremarkdown",
        "description": "Create a dcc.Markdown element",
        "body": [
			"dcc.Markdown(id='$1', className='$2', '''",
			"\t$3",
			"''')$0"
        ]
	},
	"Dash Core Component - Radio Items": {
        "prefix": "dashcoreradioitems",
        "description": "Create a dcc.RadioItems element",
        "body": [
			"dcc.RadioItems(id='$1', className='$2',",
            "\toptions=[$4],",
			"\tvalue='$5'",
            ")$0"
        ]
	},
	"Dash Core Component - RangeSlider": {
        "prefix": "dashcorerangeslider",
        "description": "Create a dcc.RangeSlider element",
        "body": [
            "dcc.RangeSlider(id='$1', className='$2',",
            "\tmin=$3, max=$4,",
			"\tstep=$5,",
			"\t#marks={$6}",
			"\tcount=$7,",
			"\tvalue=[$8, $9]",
            ")$0"
        ]
	},
	"Dash Core Component - Slider": {
        "prefix": "dashcoreslider",
        "description": "Create a dcc.Slider element",
        "body": [
            "dcc.Slider(id='$1', className='$2',",
            "\tmin=$3, max=$4,",
			"\tstep=$5,",
			"\tvalue=$6",
            ")$0"
        ]
	},
	"Dash Core Component - Tab": {
        "prefix": "dashcoretab",
        "description": "Create a dcc.Tab element",
        "body": [
			"dcc.Tab(id='$1', className='$2',",
			"\tlabel='$3', value='$4')$0"
        ]
	},
	"Dash Core Component - Tabs": {
        "prefix": "dashcoretabs",
        "description": "Create a dcc.Tabs element",
        "body": [
			"dcc.Tabs(id='$1', className='$2', value='$3', children=[",
			"\t$4",
			"])$0"
        ]
	},
	"Dash Core Component - Textarea": {
        "prefix": "dashcoretextarea",
        "description": "Create a dcc.Textarea element",
        "body": [
			"dcc.Textarea(id='$1', className='$2',",
            "\tplaceholder='$4',",
			"\tvalue='$5'",
            ")$0"
        ]
	}
}